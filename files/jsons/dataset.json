u0627\u062f\u0647 \u0648 \u0648\u06cc\u0648 \u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0631\u0648 \u0628\u0627\u0647\u0645 \u0628\u0631\u0631\u0633\u06cc \u0645\u06cc \u06a9\u0646\u06cc\u0645 \u060c \u062f\u0631 \u0627\u062f\u0627\u0645\u0647 \u0628\u0647 \u0646\u062d\u0648\u0647 \u0633\u0627\u062e\u062a \u0648 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u0635\u0641\u062d\u0647 \u0647\u0627\u06cc \u0645\u062e\u062a\u0644\u0641 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0645\u06cc \u067e\u0631\u062f\u0627\u0632\u06cc\u0645 \u0648 \u0628\u0639\u062f \u0627\u0632 \u0627\u0648\u0646 \u0628\u0627 \u06a9\u0644\u0627\u0633 \u0647\u0627\u06cc \u0645\u0647\u0645 \u0630\u062e\u06cc\u0631\u0647 \u0627\u0637\u0644\u0627\u0639\u0627\u062a \u06a9\u0627\u0631 \u0645\u06cc\u06a9\u0646\u06cc\u0645 \u060c.\u0633\u067e\u0633 \u0645\u06cc\u0631\u06cc\u0645 \u0633\u0631\u0627\u063a \u0627\u0646\u0648\u0627\u0639 \u0644\u06cc\u0633\u062a \u0647\u0627 \u06a9\u0647 \u0634\u0627\u0645\u0644 \u0644\u06cc\u0633\u062a \u0647\u0627\u06cc \u0633\u0627\u062f\u0647 \u0648 \u0633\u0641\u0627\u0631\u0634\u06cc \u0648 \u0631\u06cc\u0633\u0627\u06cc\u06a9\u0644\u0631 \u0647\u0627 \u0645\u06cc\u0634\u0647 (\u0631\u06cc\u0633\u0627\u06cc\u06a9\u0644\u0631 \u0647\u0627 \u0646\u0648\u0639 \u062e\u0627\u0635 \u0648 \u0642\u0648\u06cc \u062a\u0631\u06cc \u0627\u0632 \u0644\u06cc\u0633\u062a \u0647\u0627 \u0647\u0633\u062a\u0646\u062f) \u0648 \u0628\u0639\u062f \u0627\u0632 \u0627\u0646\u062c\u0627\u0645 \u0647\u0645\u0647 \u0627\u06cc\u0646 \u06a9\u0627\u0631\u0627 \u0627\u06a9\u0634\u0646 \u0628\u0627\u0631 \u067e\u06cc\u0634 \u0641\u0631\u0636 \u0648 \u0633\u0641\u0627\u0631\u0634\u06cc \u0631\u0648 \u0628\u0627\u0647\u0645 \u0628\u0631\u0631\u0633\u06cc \u0645\u06cc\u06a9\u0646\u06cc\u0645 \u0648 \u062f\u0631 \u0622\u062e\u0631\u06cc\u0646 \u0628\u062e\u0634 \u0622\u0645\u0648\u0632\u0634 \u0628\u0647 \u0646\u062d\u0648\u0647 \u0633\u0627\u062e\u062a \u0648 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0627\u0632 \u06a9\u062a\u0627\u0628\u062e\u0627\u0646\u0647 \u0647\u0627 \u0645\u06cc \u067e\u0631\u062f\u0627\u0632\u06cc\u0645 .\u0628\u0639\u062f \u0627\u0632 \u06af\u0632\u0631\u0648\u0646\u062f\u0646 \u0627\u06cc\u0646 \u0645\u0631\u0627\u062d\u0644 \u0648\u0627\u0631\u062f \u0641\u0627\u0632 \u062c\u062f\u06cc\u062f \u06a9\u0627\u0631 \u06cc\u0639\u0646\u06cc \u0633\u0627\u062e\u062a \u067e\u0631\u0648\u0698\u0647 \u0639\u0645\u0644\u06cc \u0627\u0635\u0644\u06cc \u0645\u0648\u0646 \u06a9\u0647 \u0633\u0627\u062e\u062a \u062f\u0641\u062a\u0631 \u062a\u0644\u0641\u0646 \u0647\u0633\u062a \u0645\u06cc\u0634\u06cc\u0645 \u0648 \u0627\u0648\u0646 \u0631\u0648 \u0628\u0627 \u0647\u0645 \u062f\u06cc\u06af\u0647 \u0645\u06cc \u0633\u0627\u0632\u06cc\u0645.\u00a0\u0627\u0645\u06cc\u062f\u0648\u0627\u0631\u0645 \u0627\u0632 \u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0627\u0633\u062a\u0641\u0627\u062f\u0647 \u0628\u0628\u0631\u06cc\u062f .\u00a0\u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0628\u0647 \u0627\u062a\u0645\u0627\u0645 \u0631\u0633\u06cc\u062f\u0647 \u0627\u0633\u062a .\u00a0", "tags": "\u0627\u0645\u0648\u0632\u0634 \u062c\u0627\u0648\u0627 \u0622\u0645\u0648\u0632\u0634 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u06cc\u0627\u062f\u06af\u06cc\u0631\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0622\u0645\u0648\u0632\u0634 android \u0622\u0645\u0648\u0632\u0634 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0627\u0633\u062a\u0648\u062f\u06cc\u0648"}, {"id": 9, "link": "https://toplearn.com/courses/mobile/\u062f\u0648\u0631\u0647-\u0628\u0631\u0646\u0627\u0645\u0647-\u0646\u0648\u06cc\u0633\u06cc-\u0627\u0646\u062f\u0631\u0648\u06cc\u062f-\u067e\u06cc\u0634\u0631\u0641\u062a\u0647", "title": "\u062f\u0648\u0631\u0647 \u0628\u0631\u0646\u0627\u0645\u0647 \u0646\u0648\u06cc\u0633\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u067e\u06cc\u0634\u0631\u0641\u062a\u0647", "plot": "\u062f\u0648\u0631\u0647 \u0628\u0631\u0646\u0627\u0645\u0647 \u0646\u0648\u06cc\u0633\u06cc android \u067e\u06cc\u0634\u0631\u0641\u062a\u0647 \u0628\u0647 \u0647\u0645\u0631\u0627\u0647 \u067e\u0631\u0648\u0698\u0647 \u0639\u0645\u0644\u06cc \u0627\u06cc\u0646\u0633\u062a\u0627\u06af\u0631\u0627\u0645\u00a0\u00a0\u062f\u0631 \u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0642\u0631\u0627\u0631 \u0647\u0633\u062a \u0627\u0631\u062a\u0628\u0627\u0637 \u0628\u0627 \u0633\u0631\u0648\u0631 \u062f\u0631 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0631\u0648 \u06a9\u0627\u0631 \u06a9\u0646\u06cc\u0645\u00a0\u0632\u0628\u0627\u0646 \u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u06a9\u0627\u062a\u0644\u06cc\u0646 \u0647\u0633\u062a \u00a0\u0648 \u067e\u0631\u0648\u0698\u0647 \u0639\u0645\u0644\u06cc \u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0645\u0633\u0646\u062c\u0631\u06cc \u0634\u0628\u06cc\u0647 \u0628\u0647 \u0627\u06cc\u0646\u0633\u062a\u0627 \u0628\u0627\u0642\u0627\u0628\u0644\u06cc\u062a \u0647\u0627\u06cc \u0632\u06cc\u0631 \u0647\u0633\u062a1) \u067e\u0633\u062a \u06af\u0630\u0627\u0634\u062a\u0646\u00a02) \u0644\u0627\u06cc\u06a9 \u06a9\u0631\u062f\u0646 \u067e\u0633\u062a \u0647\u0627\u00a03) \u0646\u0638\u0631 \u062f\u0627\u062f\u0646\u00a04) \u062f\u0627\u0634\u062a\u0646 \u067e\u0631\u0648\u0641\u0627\u06cc\u0644 \u0627\u062e\u062a\u0635\u0627\u0635\u06cc \u0628\u0631\u0627\u06cc \u0647\u0631\u06a9\u0627\u0628\u0631 \u0648 \u0642\u0627\u0628\u0644\u06cc\u062a \u0648\u06cc\u0631\u0627\u06cc\u0634 \u0622\u0646\u00a05) \u062c\u0633\u062a\u062c\u0648 \u06a9\u0627\u0631\u0628\u0631\u0627\u0646\u0628\u0639\u062f \u0627\u0632 \u06af\u0632\u0631\u0648\u0646\u062f\u0646 \u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0634\u0645\u0627 \u0645\u06cc\u062a\u0648\u0646\u06cc\u062f \u0642\u0627\u0628\u0644\u06cc\u062a \u0647\u0627\u06cc\u06cc \u0645\u062b\u0644 \u0627\u0633\u062a\u0648\u0631\u06cc \u06af\u0630\u0627\u0634\u062a\u0646 \u0631\u0648 \u062e\u0648\u062f\u062a\u0648\u0646 \u067e\u06cc\u0627\u062f\u0647 \u0633\u0627\u0632\u06cc \u06a9\u0646\u06cc\u062f \u0648 \u0647\u0645\u0686\u0646\u06cc\u0646 \u0645\u06cc\u062a\u0648\u0646\u06cc\u062f \u0628\u0631\u0646\u0627\u0645\u0647 \u0647\u0627\u06cc\u06cc \u0645\u062b\u0644 \u0648\u0627\u062a\u0633\u0627\u067e \u0648 \u062a\u0644\u06af\u0631\u0627\u0645 \u0631\u0648 \u06a9\u0647 \u0646\u0633\u0628\u062a \u0628\u0647 \u0627\u06cc\u0646\u0633\u062a\u0627 \u067e\u06cc\u0634\u0631\u0641\u062a\u0647 \u062a\u0631 \u0647\u0633\u062a\u0646\u062f \u0631\u0648 \u0628\u0633\u0627\u0632\u06cc\u062f\u0628\u06cc\u0627\u06cc\u062f \u0634\u0631\u0648\u0639 \u06a9\u0646\u06cc\u0645\u067e\u06cc\u0634\u0646\u06cc\u0627\u0632 \u062f\u0648\u0631\u0647 : \u0628\u0631\u0646\u0627\u0645\u0647 \u0646\u0648\u06cc\u0633\u06cc \u0645\u0642\u062f\u0645\u0627\u062a\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f\u062a\u0648\u062c\u0647 : \u0632\u0628\u0627\u0646 \u062f\u0648\u0631\u0647 \u06a9\u0627\u062a\u0644\u06cc\u0646 \u0627\u0633\u062a\u0627\u06cc\u0646 \u062f\u0648\u0631\u0647 \u0628\u0647 \u0627\u062a\u0645\u0627\u0645 \u0631\u0633\u06cc\u062f\u0647 \u0627\u0633\u062a .\u00a0\u00a0", "tags": "\u0628\u0631\u0646\u0627\u0645\u0647 \u0646\u0648\u06cc\u0633\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0628\u0631\u0646\u0627\u0645\u0647 \u0646\u0648\u06cc\u0633\u06cc \u067e\u06cc\u0634\u0631\u0641\u062a\u0647 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0627\u0631\u062a\u0628\u0627\u0637 \u0628\u0627 \u0633\u0631\u0648\u0631 \u062f\u0631 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0633\u0627\u062e\u062a \u0628\u0631\u0646\u0627\u0645\u0647 \u0628\u0627 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0627\u0633\u062a\u0648\u062f\u06cc\u0648 \u0627\u0645\u0648\u0632\u0634 \u062d\u0631\u0641\u0647 \u0627\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0627\u0645\u0648\u0632\u0634 \u06a9\u0627\u062a\u0644\u06cc\u0646 \u0628\u0631\u0627\u06cc \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0622\u0645\u0648\u0632\u0634 \u06a9\u0627\u062a\u0644\u06cc\u0646 \u062f\u0631 \u0627\u0646\u062f\u0631\u0648\u06cc\u062f \u0627\u0633\u062a\u0648\u062f\u06cc\u0648"}]